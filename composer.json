{
  "name": "peibin/laravel-coroutine",
  "description": "Peibin coroutine component for Laravel.",
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "laravel",
    "coroutine"
  ],
  "authors": [
    {
      "name": "Peibin Zhu",
      "email": "zhupeibin1207@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "peibin/laravel-contracts": "^1.0",
    "peibin/laravel-engine": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PeibinLaravel\\Coroutine\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PeibinLaravelTest\\Coroutine\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true
}
